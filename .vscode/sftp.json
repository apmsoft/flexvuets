{
    "name": "My Server",
    "host": "localhost",
    "protocol": "sftp",
    "port": 22,
    "username": "username",
    "remotePath": "/",
    "uploadOnSave": false,
    "useTempFile": false,
    "openSsh": false,
    "ignore": [
        ".DS_Store",
        ".vscode",
        ".git",
        "**/.*",
        "**/*.md",
        "node_modules/**",
        "!release/**"
    ],
    "include": [
        "release/**"
    ],
    "watcher": {
        "files": "release/**/*",
        "autoUpload": true,
        "autoDelete": false
    },
    "syncMode": "update"
}
