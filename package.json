{
  "name": "flexvue",
  "version": "2.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "debug": "tsc -P ./tsconfig.json && babel debug -d debug",
    "build": "tsc -P ./tsconfig.json && babel debug -d debug && export-plugins",
    "start": "tsc -P ./tsconfig.json --watch && babel debug --out-dir debug --watch",
    "export-plugins": "node export-plugins.js",
    "release": "cp -fr ./debug/* ./release/v1",
    "clean": "tsc --build --clean",
    "clean-dev": "rm -fr ./release/v1/*",
    "clean-release": "rm -fr ./release/v1/*",
    "css": "npx tailwindcss -i ./tailwind.css -o ./debug/example/css/tailwind.css --watch --minify"
  },
  "author": "fancyupsoft.com",
  "license": "ISC",
  "dependencies": {
    "@editorjs/editorjs": "^2.29.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@editorjs/checklist": "^1.6.0",
    "@editorjs/code": "^2.9.0",
    "@editorjs/embed": "^2.7.0",
    "@editorjs/header": "^2.8.1",
    "@editorjs/image": "^2.9.0",
    "@editorjs/link": "^2.6.2",
    "@editorjs/list": "^1.9.0",
    "@editorjs/quote": "^2.6.0",
    "@editorjs/simple-image": "^1.6.0",
    "@editorjs/table": "^2.3.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.6",
    "@types/jquery": "^3.5.22",
    "@types/node": "^20.8.6",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "filepond": "^4.30.6",
    "filepond-plugin-file-validate-size": "^2.2.8",
    "filepond-plugin-file-validate-type": "^1.2.9",
    "filepond-plugin-image-preview": "^4.6.12",
    "filepond-plugin-image-validate-size": "^1.2.7",
    "fs-extra": "^11.2.0",
    "postcss": "^8.4.31",
    "swiper": "^11.0.7",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
