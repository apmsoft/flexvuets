{
  "sourceMaps": false,
  "retainLines": true,
  "presets": ["@babel/preset-typescript"],
  "plugins": [
    [ "module-resolver", {
        "alias": {
          "src": "./debug"
          ,"@flexvue":"./debug/flexvue"
          ,"@editorjs/editorjs": "./debug/plugins/@editorjs/editorjs/dist/editorjs.mjs"
